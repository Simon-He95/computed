{
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        [
            "module-resolver",
            {
                "root": [
                    "./src"
                ],
                "alias": {}
            }
        ]
    ],
    "presets": [
        "@babel/typescript",
        [
            "@babel/env",
            {
                "targets": {
                    "chrome": "67"
                },
                "loose": false,
                "useBuiltIns": "entry",
                "corejs": {
                    "version": 3,
                    "proposals": true
                },
                "modules": "commonjs",
            }
        ]
    ]
}